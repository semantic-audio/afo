    <rdfs:comment xml:lang="en">PitchContoursMonoMelody Extractor</rdfs:comment>
    <afo:input rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/Contoursstarttimes"/>
    <afo:parameter rdf:nodeID="N23427052fa43485898b7bde17a1c9eb6"/>
    <rdfs:subClassOf rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchExtractors"/>
    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchContoursMonoMelodyPitch"/>
    <afo:input rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/Contoursbins"/>
    <afo:input rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/Contourssaliences"/>
    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchContoursMonoMelodyPitchconfidence"/>
