    <rdfs:subClassOf rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/TonalExtractors"/>
    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/ChordsStrength"/>
    <rdfs:comment xml:lang="en">ChordsDetection Extractor</rdfs:comment>
