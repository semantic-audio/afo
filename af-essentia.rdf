    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/LoudnessEBUR128Loudnessrange"/>
    <afo:parameter rdf:nodeID="Nab4e39669fd04301b6412ec763202dca"/>
    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/LoudnessEBUR128Integratedloudness"/>
