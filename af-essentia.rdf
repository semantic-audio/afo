  <rdf:Description rdf:about="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/NoveltyCurveFixedBpmEstimatorExtractor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/RhythmExtractors"/>
