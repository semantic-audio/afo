  <rdf:Description rdf:about="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PredominantPitchMelodiaExtractor">
    <afo:parameter rdf:nodeID="N49f63a09997a4192ba1fe3ad289d5572"/>
    <rdfs:label xml:lang="en">PredominantPitchMelodia Extractor</rdfs:label>
    <afo:parameter rdf:nodeID="N47d1a41b08b64c84830de35bddb07122"/>
    <afo:parameter rdf:nodeID="Nb93e1a17671041d39b9409575a37ba64"/>
