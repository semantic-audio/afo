    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/SpectralPeaksMagnitudes"/>
    <rdfs:subClassOf rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/SpectralExtractors"/>
  </rdf:Description>
