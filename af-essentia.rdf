    <rdfs:label xml:lang="en">ChordsDescriptors Extractor</rdfs:label>
    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/ChordsDescriptorsChordskey"/>
