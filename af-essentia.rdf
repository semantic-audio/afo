    <afo:parameter rdf:nodeID="Nf26b7eab8e9c48dd8c7db50b9dfc1ddd"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchContoursExtractor">
