    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchYinFFTPitchconfidence"/>
    <rdfs:label xml:lang="en">PitchYinFFT Extractor</rdfs:label>
    <afo:parameter rdf:nodeID="N53f37843c8204d61a5df911455f28ce2"/>
    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchYinFFTPitch"/>
