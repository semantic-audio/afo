  <rdf:Description rdf:about="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/TempoTapMaxAgreementExtractor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
