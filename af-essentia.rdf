  <rdf:Description rdf:about="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchMelodiaExtractor">
    <afo:parameter rdf:nodeID="Necf84b1101414d43ba00468198e60c6b"/>
