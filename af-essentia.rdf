    <afo:input rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/Detections"/>
    <rdfs:label xml:lang="en">Onsets Extractor</rdfs:label>
    <afo:input rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/Weights"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
