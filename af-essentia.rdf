  <rdf:Description rdf:about="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchContoursMonoMelodyExtractor">
    <afo:parameter rdf:nodeID="N6b62983aa3ea4dd1b23fad3d4e91011e"/>
    <afo:parameter rdf:nodeID="N2af29b677bd549b08542d72dd85293f9"/>
