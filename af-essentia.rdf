  <rdf:Description rdf:about="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/LogAttackTimeExtractor">
    <rdfs:label xml:lang="en">LogAttackTime Extractor</rdfs:label>
    <afo:parameter rdf:nodeID="N6e274c29550a41efb0749b8a121894c8"/>
    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/LogAttackTime"/>
