    <afo:input rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/Frequencies"/>
    <afo:output rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/Tristimulus"/>
    <rdfs:comment xml:lang="en">Tristimulus Extractor</rdfs:comment>
    <rdfs:label xml:lang="en">Tristimulus Extractor</rdfs:label>
  </rdf:Description>
