  <rdf:Description rdf:about="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/PitchYinFFTExtractor">
    <afo:parameter rdf:nodeID="N73f286a476bc40318884fcbaadb84708"/>
    <afo:input rdf:resource="http://sovarr.c4dm.eecs.qmul.ac.uk/af/essentia/Spectrum"/>
